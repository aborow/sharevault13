<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!--SV-256-->
        <record model="ir.ui.view" id="inherit_view_partner_property_form">
            <field name="name">inherit.view.partner.property.form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="account.view_partner_property_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='accounting_entries']" position="inside">
                    <field name="supplier_rank" invisible="1"/>
                    <field name="vat" attrs="{'invisible': [('company_type','!=','company')]}"/>
                    <field name="print_on_check_as"
                            attrs="{'invisible':['|',('supplier_rank','=',False),('company_type','!=','company')]}"/>
                    <field name="eligible_1099"
                            attrs="{'invisible':['|',('supplier_rank','=',False),('company_type','!=','company')]}"/>
                    <field name="vendor_type"
                            attrs="{'invisible':['|',('supplier_rank','=',False),('company_type','!=','company')]}"/>
                </xpath>
            </field>
        </record>

        <!-- SV-98 -->
        <record id="base_partner_merge_automatic_wizard_form" model="ir.ui.view">
            <field name="name">Merge wizard</field>
            <field name="model">base.partner.merge.automatic.wizard</field>
            <field name="inherit_id" ref="base.base_partner_merge_automatic_wizard_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='partner_ids']/tree/field[@name='display_name']" position="after">
                    <field name="domain"/>
                </xpath>
            </field>
        </record>

        <!-- SV-95 -->
        <act_window id="call_dedup"
            name="Deduplication"
            binding_model="res.partner"
            res_model="base.partner.merge.automatic.wizard"
            view_mode="form"
            target="new"
            groups="base.group_partner_manager"
            context="{'active_test': False}"/>
        <menuitem id="partner_dedup"
                name="Deduplication"
                parent="contacts.res_partner_menu_config"
                action="call_dedup"
                sequence="9"/>

        <!-- SV-93 -->
        <record id="res_partner_kanban_view" model="ir.ui.view">
            <field name="name">Partner</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.res_partner_kanban_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='type']" position="after">
                    <field name="domain"/>
                </xpath>

                <xpath expr="//li[@t-if='record.email.raw_value']" position="replace">
                    <li t-if="record.email.raw_value and !record.is_company.raw_value" class="o_text_overflow"><field name="email"/></li>
                    <li t-if="record.domain.raw_value and record.is_company.raw_value" class="o_text_overflow"><field name="domain"/></li>
                </xpath>
            </field>
        </record>

        <record id="view_res_partner_filter" model="ir.ui.view">
            <field name="name">Partner</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_res_partner_filter"/>
            <field name="arch" type="xml">
                <xpath expr="//field[last()]" position="after">
                    <field name="first_name"/>
                    <field name="last_name"/>
                    <field name="contact_type_id"/>
                    <field name="print_on_check_as"/>
                    <field name="vendor_type"/>
                </xpath>

                <xpath expr="//group" position="inside">
                    <filter name="eligible_1099" domain="[]"
                            string="Eligible for 1099"
                            context="{'group_by' : 'eligible_1099'}"/>
                    <filter name="vendor_type" domain="[]"
                            string="Vendor Type"
                            context="{'group_by' : 'vendor_type'}"/>
                </xpath>
            </field>
        </record>
        <!--Sales Button of Button Box-->
        <record id="view_res_partner_buttons_inherited" model="ir.ui.view">
            <field name="name">Partner Inherited for hide buttons</field>
            <field name="model">res.partner</field>
            <field name="priority">3</field>
            <field name="inherit_id" ref="sale.res_partner_view_buttons"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='%(sale.act_res_partner_2_sale_order)d']" position="attributes">
                    <attribute name="attrs">{'invisible': [('company_type','!=','company')]}</attribute>
                </xpath>
            </field>
        </record>
        <!--Invoices Button of Button Box-->
        <record id="view_res_partner_buttons_inherited_for_iv" model="ir.ui.view">
            <field name="name">Partner Inherited for hide button of Invoiced</field>
            <field name="model">res.partner</field>
            <field name="priority">11</field>
            <field name="inherit_id" ref="account.partner_view_buttons"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='action_view_partner_invoices']" position="attributes">
                    <attribute name="attrs">{'invisible': [('company_type','!=','company')]}</attribute>
                </xpath>
            </field>
        </record>
        <!--Partner Ledger Button of Button Box-->
        <record id="view_res_partner_buttons_inherited_for_pl" model="ir.ui.view">
            <field name="name">Partner Inherited for hide button of PL</field>
            <field name="model">res.partner</field>
            <field name="priority">13</field>
            <field name="inherit_id" ref="account_reports.partner_view_buttons"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='open_partner_ledger']" position="attributes">
                    <attribute name="attrs">{'invisible': [('company_type','!=','company')]}</attribute>
                </xpath>
            </field>
        </record>
        <!--Website Button of Button Box-->
        <record id="view_res_partner_buttons_inherited_for_web" model="ir.ui.view">
            <field name="name">Partner Inherited for hide button of gtw</field>
            <field name="model">res.partner</field>
            <field name="priority">17</field>
            <field name="inherit_id" ref="website_partner.view_partners_form_website"/>
            <field name="arch" type="xml">
                <field name="is_published" position="attributes">
                    <attribute name="attrs">{'invisible': [('company_type','!=','company')]}</attribute>
                </field>
            </field>
        </record>

        <!-- SV-49 Related changes -->
        <record id="add_fields_partner_form_view" model="ir.ui.view">
            <field name="name">Partner</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@name='button_box']" position="inside">
                    <button icon="fa-user" class="oe_left" style="width: 100px !important;"
                        attrs="{'invisible':[('company_type','=','company')]}" string="Individual">
                    </button>
                    <button icon="fa-building-o" class="oe_left" style="width: 100px !important;"
                        attrs="{'invisible':[('company_type','!=','company')]}" string="Company">
                    </button>
                </xpath>
                <field name="category_id" position="after">
                    <field name="source"/>
                    <field name="is_domain_updated" invisible="1"/>
                    <button name="update_domain" string="Update Domain" type="object" class="oe_highlight" invisible="1"/>
                </field>
                <!--SV-53: Hided for individual company type-->
                <field name="website" position="attributes">
                    <attribute name="attrs">{'invisible': [('is_company', '=', False)]}</attribute>
                </field>
                <field name="industry_id" position="attributes">
                    <attribute name="attrs">{'invisible': [('is_company', '=', False)]}</attribute>
                </field>
                <!--SV-256-->
                <field name="vat" position="attributes">
                    <attribute name="invisible">1</attribute>
                </field>
                <!--SV-SV-156-->
                <field name="type" position="attributes">
                    <attribute name="invisible">1</attribute>
                </field>
                <!--SV-170-->
                <field name="type" position="after">
                    <field name="domain"/>
                </field>
            </field>
        </record>

        <!--Overrided the SMS module View for invisible the mobile label-->
        <record id="sms.res_partner_view_form" model="ir.ui.view">
            <field name="name">res.partner.view.form.inherit.sms</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="priority">10</field>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='mobile']" position="after">
                    <field name="phone_sanitized" groups="base.group_no_one" invisible="1"/>
                </xpath>
                <xpath expr="//field[@name='phone']" position="replace">
                    <label for="phone"/>
                    <div class="o_row">
                        <field name="phone" widget="phone" options="{'enable_sms': True}"/>
                    </div>
                </xpath>
                <xpath expr="//field[@name='mobile']" position="replace">
                    <label for="mobile" attrs="{'invisible': [('is_company', '=', True)]}"/>
                    <div class="o_row">
                        <field name="mobile" widget="phone"
                               options="{'enable_sms': True}"
                               attrs="{'invisible': [('is_company', '=', True)]}"/>
                    </div>
                </xpath>
            </field>
        </record>

        <!--Inherit sms view-->
        <record id="sms_res_partner_view_form" model="ir.ui.view">
            <field name="name">res.partner.view.form.inherit.sms.for.imported.phone</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="sms.res_partner_view_form"/>
            <field name="priority">10</field>
            <field name="arch" type="xml">
                <xpath expr="//form/sheet/group/group[2]/div[1]" position="after">
                    <label for="imported_phone"/>
                    <div class="o_row">
                        <field name="imported_phone" widget="phone" options="{'enable_sms': True}"/>
                    </div>
                </xpath>
            </field>
        </record>

        <!--Overrided from email module for invisible the email label-->
        <record id="mail.res_partner_view_form_inherit_mail" model="ir.ui.view">
            <field name="name">res.partner.view.form.inherit.mail</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='email']" position="replace">
                    <field name="is_blacklisted" invisible="1"/>
                    <label for="email" class="oe_inline" attrs="{'invisible': [('is_company', '=', True)]}"/>
                    <div class="o_row o_row_readonly">
                        <i class="fa fa-ban" style="color: red;" role="img" title="This email is blacklisted for mass mailing"
                            aria-label="Blacklisted" attrs="{'invisible': [('is_blacklisted', '=', False)]}" groups="base.group_user"></i>
                        <field name="email" widget="email" attrs="{'invisible': [('is_company', '=', True)]}"/>
                    </div>
                </xpath>
                <xpath expr="//sheet" position="after">
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="activity_ids" widget="mail_activity"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </xpath>
            </field>
        </record>

        <record model="ir.ui.view" id="view_partner_form_inherit_from_mail_sv">
            <field name="name">Partner</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="mail.res_partner_view_form_inherit_mail"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='email']" position="replace">
                        <label for="email" class="oe_inline" invisible="1"/>
                        <div>
                            <t attrs="{'invisible':[('is_company', '=', True)]}">
                            <table>
                                <tr>
                                    <td><field name="email"/></td>
                                    <td> </td>
                                    <td>
                                        (
                                        <field name="opt_out"/>
                                        <label for="opt_out" string="Opt Out"/>
                                        )
                                    </td>
                                </tr>
                            </table>
                            </t>
                        </div>
                </xpath>
            </field>
        </record>

        <record model="ir.ui.view" id="view_partner_form_inherit">
            <field name="name">Partner</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='vat']" position="after">
                    <field name="sharevault_ids" invisible="1"/>
                    <field name="supplier_rank" invisible="1"/>
                </xpath>

                <xpath expr="//div[@name='button_box']/button[last()]" position="before">
                    <button name="get_sharevault"
                        type="object"
                        context="{'search_default_partner_id': active_id, 'default_partner_id': active_id}"
                        class="oe_stat_button" icon="fa-archive"
                        attrs="{'invisible':[('sharevault_ids','=',[])]}">
                        <field name="sharevault_ids_count" string="ShareVaults"
                                widget="statinfo" />
                    </button>
                    <!--SV-75-->
                    <button name="get_sharevault_for_company"
                        type="object"
                        context="{'search_default_partner_id': active_id, 'default_partner_id': active_id}"
                        class="oe_stat_button" icon="fa-archive"
                        attrs="{'invisible':[('sharevault_company_count','=',0)]}">
                        <field name="sharevault_company_count" string="ShareVaults"
                                widget="statinfo" />
                    </button>
                    <!-- show auditlog -->
                    <!--
                    <button name="call_auditlog"
                        type="object"
                        class="oe_stat_button" icon="fa-bars"
                        attrs="{'invisible':[('auditlog_ids_count','=',0)]}">
                        <field name="auditlog_ids_count" string="Logs"
                                widget="statinfo" />
                    </button>
                    -->
                </xpath>

                <xpath expr="//field[@name='name']" position="after">
                    <field name="first_name" invisible="1"
                            readonly="1" force_save="1"/>
                    <field name="last_name" invisible="1"
                            readonly="1" force_save="1"/>
                </xpath>


                <xpath expr="//field[@name='parent_id']" position="before">
                    <!--SV-77-->
                    <t attrs="{'invisible':[('company_type','!=','company')]}">
                        (<label for="contact_type_id"/> <field name="contact_type_id"/>)
                    </t>
                </xpath>

                <xpath expr="//field[@name='function']" position="after">
                    <label for="fax"/>
                    <div>
                        <table>
                            <tr>
                                <td><field name="fax"/></td>
                                <td> </td>
                                <!-- SV-102 : Opt Out field invisible for Company Contact -->
                                <t attrs="{'invisible':[('is_company', '=', True)]}">
                                    <td >
                                        (
                                        <field name="fax_opt_out"/>
                                        <label for="fax_opt_out" string="Opt Out"/>
                                        )
                                    </td>
                                </t>
                            </tr>
                        </table>
                    </div>
                </xpath>

                <xpath expr="//field[@name='function']" position="replace"/>

                <xpath expr="//field[@name='vat']" position="after">
                    <field name="function" attrs="{'invisible':[('company_type','=','company')]}"/>
                    <field name="job_function" attrs="{'invisible':[('company_type','=','company')]}"/>
                    <field name="job_level" attrs="{'invisible':[('company_type','=','company')]}"/>

                </xpath>

                <xpath expr="//page[last()]" position="after">
                    <!--SV-76-->
                    <page string="SV User Info" attrs="{'invisible':[('company_type','=','company')]}">
                        <group>
                            <group>
                                <field name="sharevault_last_login_date"
                                        string="Last Login"/>
                                <field name="sharevault_subscription"
                                        string="Subscription"/>
                                <field name="sharevault_user"
                                        string="User"/>
                                <field name="sharevault_admin"
                                        string="Admin"/>
                            </group>
                            <group>
                                <field name="sharevault_activated_user"
                                        string="Activated User"/>
                                <field name="sharevault_domain"
                                        string="Domain"/>
                                <field name="sharevault_email_subscription"
                                        string="Email Subscription"/>
                                <field name="sharevault_publisher"
                                        string="Publisher"/>
                            </group>
                        </group>
                    </page>
                    <page string="ShareVaults" attrs="{'invisible':[('company_type','!=','company')]}">
                        <field name="sharevault_c_ids" nolabel="1">
                            <tree string="Sharevaults" readonly="1">
                                <field name="sharevault_name"/>
                                <field name="sharevault_owner"/>
                                <field name="sv_type"/>
                            </tree>
                        </field>
                    </page>
                </xpath>
                <xpath expr="//page[1]" position="after">
                        <page string="Additional Info">
                            <group>
                                <group>
                                    <field name="ae_targeted"/>
                                    <!--Hided for individual company type-->
                                    <!--
                                    <field name="annual_revenue" attrs="{'invisible': [('is_company', '=', False)]}"/>
                                    -->
                                    <field name="european_union" attrs="{'invisible': [('is_company', '!=', False)]}"/>
                                    <field name="data_source_details"/>
                                    <!--<field name="imported_phone"/>-->
                                    <!--Hided for individual company type-->
                                    <field name="number_employees" attrs="{'invisible': [('is_company', '=', False)]}"/>
                                </group>
                                <group>
                                    <field name="marketing_flag"/>
                                    <field name="marketing_note"/>
                                    <field name="salesforce_contact_id" attrs="{'invisible': [('is_company', '!=', False)]}"/>
                                    <field name="salesforce_lead_id" attrs="{'invisible': [('is_company', '!=', False)]}"/>
                                    <field name="agree_data_collection"/>
                                </group>
                            </group>

                            <group>
                                <group>
                                    <!-- SV-102 : Account Status field invisible for Individual Contact -->
                                    <field name="account_status_id"
                                        attrs="{'invisible': [('is_company', '=', False)]}"/>
                                    <field name="data_source_id"/>
                                    <field name="data_update_request_id"/>
                                    <field name="lead_type_id"/>
                                    <field name="lifecycle_stage_id"/>
                                    <field name="organization_type_id" attrs="{'invisible': [('is_company', '=', False)]}"/>
                                </group>
                                <group>
                                    <field name="persona_id" attrs="{'invisible': [('is_company', '!=', False)]}"/>
                                    <field name="status_id" string="Status (Company)" attrs="{'invisible': [('is_company', '=', False)]}"/>
                                    <field name="status_id" string="Status (Individual)" attrs="{'invisible': [('is_company', '!=', False)]}"/>
                                    <field name="tz"/>
                                    <!-- Sub industry comes based on Industry-->
                                    <field name="subindustry_id" domain="[('industry_id', '=', industry_id)]"/>
                                    <field name="accindustry_id" attrs="{'invisible': [('is_company', '=', False)]}"
                                           options="{'no_create': True, 'no_create_edit':True}"/>
                                    <!-- SV-68 Create new Fields in Sharevault for Companies -->
                                    <field name="sic_code" attrs="{'invisible': [('is_company', '=', False)]}"/>
                                    <field name="naics_code" attrs="{'invisible': [('is_company', '=', False)]}"/>
                                    <field name="zoom_company_id" attrs="{'invisible': [('is_company', '=', False)]}"/>
                                </group>
                            </group>
                            <group>
                                <group colspan="4">
                                    <label for="matter_id" colspan="2"/>
                                    <field name="matter_id" colspan="2" nolabel="1"/>
                                    <label for="tecnology_id" colspan="2"/>
                                    <field name="tecnology_id" colspan="2" nolabel="1"/>
                                    <label for="confidential_id" colspan="2"/>
                                    <field name="confidential_id" colspan="2" nolabel="1"/>
                                </group>
                            </group>
                        </page>
                </xpath>

            </field>
        </record>

        <record id="sharevault_fields_persona_form" model="ir.ui.view">
            <field name="name">Persona</field>
            <field name="model">res.partner.persona</field>
            <field name="arch" type="xml">
                <form string="Persona">
                    <sheet>
                        <group>
                            <field name="sequence" invisible="1"/>
                            <field name="name"/>
                            <field name="active"/>
                            <field name="domain" widget="domain"
                                    options="{'model': 'res.partner'}"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="sharevault_fields_persona_tree" model="ir.ui.view">
            <field name="name">Persona</field>
            <field name="model">res.partner.persona</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="sequence" widget="handle"/>
                    <field name="name"/>
                    <field name="active"/>
                </tree>
            </field>
        </record>
        <record id="sharevault_fields_persona_action" model="ir.actions.act_window">
            <field name="name">Persona</field>
            <field name="res_model">res.partner.persona</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('active','in',[True,False])]</field>
        </record>

        <record id="sharevault_fields_job_function_tree" model="ir.ui.view">
            <field name="name">Job Function</field>
            <field name="model">res.partner.job_function</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="name"/>
                    <field name="active"/>
                </tree>
            </field>
        </record>
        <record id="sharevault_fields_job_function_action" model="ir.actions.act_window">
            <field name="name">Job Function</field>
            <field name="res_model">res.partner.job_function</field>
            <field name="view_mode">tree</field>
            <field name="domain">[('active','in',[True,False])]</field>
        </record>

        <record id="sharevault_fields_confidential_tree" model="ir.ui.view">
            <field name="name">Confidential</field>
            <field name="model">res.partner.confidential</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="name"/>
                    <field name="active"/>
                </tree>
            </field>
        </record>
        <record id="sharevault_fields_confidential_action" model="ir.actions.act_window">
            <field name="name">Confidential</field>
            <field name="res_model">res.partner.confidential</field>
            <field name="view_mode">tree</field>
            <field name="domain">[('active','in',[True,False])]</field>
        </record>

        <record id="sharevault_fields_tecnology_tree" model="ir.ui.view">
            <field name="name">Tecnology</field>
            <field name="model">res.partner.tecnology</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="name"/>
                    <field name="active"/>
                </tree>
            </field>
        </record>
        <record id="sharevault_fields_tecnology_action" model="ir.actions.act_window">
            <field name="name">Tecnology</field>
            <field name="res_model">res.partner.tecnology</field>
            <field name="view_mode">tree</field>
            <field name="domain">[('active','in',[True,False])]</field>
        </record>

        <record id="sharevault_fields_matter_tree" model="ir.ui.view">
            <field name="name">Matter</field>
            <field name="model">res.partner.matter</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="name"/>
                    <field name="active"/>
                </tree>
            </field>
        </record>
        <record id="sharevault_fields_matter_action" model="ir.actions.act_window">
            <field name="name">Matter</field>
            <field name="res_model">res.partner.matter</field>
            <field name="view_mode">tree</field>
            <field name="domain">[('active','in',[True,False])]</field>
        </record>

        <record id="sharevault_fields_contact_type_tree" model="ir.ui.view">
            <field name="name">Company Type</field>
            <field name="model">res.partner.contact_type</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="name"/>
                    <field name="active"/>
                </tree>
            </field>
        </record>
        <record id="sharevault_fields_contact_type_action" model="ir.actions.act_window">
            <field name="name">Company Type</field>
            <field name="res_model">res.partner.contact_type</field>
            <field name="view_mode">tree</field>
